{
  "expo": {
    "name": "Medinalic",
    "slug": "Medinalic",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "medinalic",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#469C98"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.Medinalic",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "we want to use camera",
        "NSMicrophoneUsageDescription": "we want to use microphone",
        "CADisableMinimumFrameDurationOnPhone": true
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:medinalic.app",
        "applinks:medinalic.dev"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Medinalic"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}